{
    "configurations": [
        {
            "name": "Linux-Clang",
            "includePath": [
                "/usr/include",
                "${workspaceFolder}/utils",
                "${workspaceFolder}/../src"
            ],
            "defines": [
                "TARGET_NANOS",
                "OS_IO_SEPROXYHAL",
                "HAVE_BAGL",
                "HAVE_ECC",
                "HAVE_ECC_WEIERSTRASS",
                "HAVE_SECP_CURVES",
                "HAVE_SPRINTF",
                "HAVE_HASH",
                "HAVE_BLAKE2",
                "HAVE_RNG",
                "HAVE_HMAC",
                "HAVE_SHA256",
                "HAVE_MATH",
                "HAVE_IO_USB",
                "HAVE_L4_USBLIB",
                "IO_USB_MAX_ENDPOINTS=6",
                "IO_HID_EP_LENGTH=64",
                "HAVE_USB_APDU",
                "USB_SEGMENT_SIZE=64",
                "UNUSED(x)=(void)x",
                "APPVERSION=\"1.0.0\"",
                "APPNAME=\"Ergo\"",
                "MAJOR_VERSION=1",
                "MINOR_VERSION=0",
                "PATCH_VERSION=0",
                "IO_SEPROXYHAL_BUFFER_SIZE_B=128",
                "HAVE_UX_FLOW",
                "DEBUG_BUILD=1",
                "HAVE_PRINTF",
                "PRINTF=screen_printf"
            ],
            "compilerPath": "/usr/bin/clang",
            "cStandard": "c99",
            "cppStandard": "c++17",
            "browse": {
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            },
            "configurationProvider": "ms-vscode.cmake-tools"
        }
    ],
    "version": 4
}